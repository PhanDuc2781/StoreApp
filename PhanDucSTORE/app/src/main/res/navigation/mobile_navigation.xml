        tools:layout="@layout/fragment_my_order" />
    <fragment
        android:id="@+id/nav_my_carts"
        android:name="com.example.phanduc_store.Fragments.MyCartFragment"
        android:label="Giở Hàng"
        tools:layout="@layout/fragment_my_cart" />
    <fragment
        android:id="@+id/nav_logOut"
        android:name="com.example.phanduc_store.Fragments.LogOutFragment"
        android:label="Đăng xuất"
        tools:layout="@layout/fragment_log_out" />
    <fragment
        android:id="@+id/nav_about"
        android:name="com.example.phanduc_store.Fragments.AboutFragment"
        android:label="Giới Thiệu"
        tools:layout="@layout/fragment_about" />


</navigation>